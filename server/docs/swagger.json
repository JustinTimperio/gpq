    "paths": {
        "/arrow/receive/{name}": {
            "post": {
                "security": [
                    {
                        "ApiKeyAuth": []
                    }
                ],
                "description": "Receives Arrow data and enqueues it into a topic",
                "consumes": [
                    "application/vnd.apache.arrow"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Arrow"
                ],
                "summary": "Receive Arrow Data",
                "operationId": "arrow-receive",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Topic Name",
                        "name": "name",
                        "in": "path",
                        "required": true
                    },
                    {
                        "type": "integer",
                        "description": "Priority",
                        "name": "priority",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "boolean",
                        "description": "Should Escalate",
                        "name": "should_escalate",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "Escalate Every",
                        "name": "escalate_every",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "boolean",
                        "description": "Can Timeout",
                        "name": "can_timeout",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "Timeout Duration",
                        "name": "timeout_duration",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "Bearer {token}",
                        "name": "Authorization",
                        "in": "header",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/arrow/serve/{name}": {
            "get": {
                "security": [
                    {
                        "ApiKeyAuth": []
                    }
                ],
                "description": "Batches and serves Arrow data from a topic",
                "produces": [
                    "application/vnd.apache.arrow.stream"
                ],
                "tags": [
                    "Arrow"
                ],
                "summary": "Serve Arrow Data",
                "operationId": "arrow-serve",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Topic Name",
                        "name": "name",
                        "in": "path",
                        "required": true
                    },
                    {
                        "type": "integer",
                        "description": "Number of records to collect",
                        "name": "records",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Bearer {token}",
                        "name": "Authorization",
                        "in": "header",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Arrow Data",
                        "schema": {
                            "type": "array",
                            "items": {
                                "type": "integer"
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
